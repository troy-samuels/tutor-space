{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "translate": "node scripts/translate-deepl.js",
    "test": "node --loader ./scripts/ts-loader.mjs --test",
    "test:unit": "node --loader ./scripts/ts-loader.mjs --test tests/unit/**/*.test.ts",
    "test:integration": "node --loader ./scripts/ts-loader.mjs --test tests/integration/**/*.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:studio": "playwright test --project=studio",
    "test:e2e:homework": "playwright test --project=homework",
    "test:e2e:practice": "playwright test --project=practice",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "smoke": "npm run lint && npm test",
    "i18n:scan": "node --loader ./scripts/ts-loader.mjs --no-warnings ./scripts/scan-hardcoded-i18n.ts",
    "dast:local": "hawk scan",
    "dast:ci": "hawk scan",
    "dast:report": "mkdir -p reports && hawk scan",
    "dast:google": "hawk scan stackhawk-google-oauth.yml"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.11.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@livekit/components-react": "^2.9.17",
    "@livekit/components-styles": "^1.2.0",
    "@livekit/track-processors": "^0.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/stripe-js": "^8.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "@tanstack/react-table": "^8.20.5",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.24",
    "gray-matter": "^4.0.3",
    "jose": "^6.1.2",
    "livekit-server-sdk": "^2.14.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.545.0",
    "next": "16.0.10",
    "next-intl": "^4.6.0",
    "openai": "^6.9.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.65.0",
    "react-resizable-panels": "^3.0.6",
    "reading-time": "^1.5.0",
    "recharts": "^3.5.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "sanitize-html": "^2.17.0",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/sanitize-html": "^2.16.0",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
